# Copy to /opt/colaba/.env on server. Fill all values.
# Get values from Coolify Environment Variables if already deployed there.

# PostgreSQL
POSTGRES_USER=leadgen_user
POSTGRES_PASSWORD=CHANGE_ME
POSTGRES_DB=leadgen_db

# Database URLs (host=postgres for docker network)
DATABASE_URL=postgresql+asyncpg://leadgen_user:CHANGE_ME@postgres:5432/leadgen_db
DATABASE_URL_SYNC=postgresql://leadgen_user:CHANGE_ME@postgres:5432/leadgen_db

# Redis
REDIS_URL=redis://redis:6379/0
CELERY_BROKER_URL=redis://redis:6379/0
CELERY_RESULT_BACKEND=redis://redis:6379/0

# App
SECRET_KEY=CHANGE_ME_OR_GENERATE_LONG_RANDOM_STRING
LOG_LEVEL=INFO

# Frontend (replace with your domains)
CORS_ORIGINS=http://your-frontend-domain.88.210.53.183.sslip.io
NEXT_PUBLIC_API_URL=http://your-backend-domain.88.210.53.183.sslip.io/api/v1

# Optional
OPENAI_API_KEY=
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=llama2
YANDEX_XML_FOLDER_ID=
YANDEX_XML_KEY=
USE_PROXY=false
PROXY_URL=
PROXY_LIST=
